{
  "name": "shellcheck-binaries",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "npm-run-all --serial check:*",
    "check:format": "prettier --check .",
    "fix": "npm-run-all --serial fix:*",
    "fix:format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-sh": "^0.18.0"
  },
  "volta": {
    "node": "24.5.0",
    "npm": "11.5.2"
  }
}
