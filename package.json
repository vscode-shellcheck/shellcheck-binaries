{
  "name": "shellcheck-binaries",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "npm-run-all --serial check:*",
    "check:format": "prettier --check .",
    "fix": "npm-run-all --serial fix:*",
    "fix:format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-sh": "^0.14.0"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.5.0"
  }
}
